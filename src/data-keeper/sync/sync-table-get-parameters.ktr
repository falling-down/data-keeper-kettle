    <connection>dw</connection>
    <sql>select
	trn.transformation_key,
	trn.source_schema,
	trn.source_table,
	trn.sync_schema,
	trn.sync_table,
	trn.source_match_id,
	trn.sync_match_id,
	trn.source_filter,
	trn.sync_filter,
	trn.source_connection_id,
	conn.connection_type,
	conn.oracle_host,
	conn.oracle_port,
	conn.oracle_database,
	conn.oracle_username,
	&apos;&apos; as oracle_password
from 
	${META_SCHEMA}.meta_sync_transformations trn
	inner join ${META_SCHEMA}.meta_connections conn
		on trn.source_connection_id = conn.connection_id
where
	trn.transformation_id = &apos;${TRANS_ID}&apos;</sql>
